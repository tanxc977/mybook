<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xc.auth.mapper.RoleMapper">
    <select id="findAllRole" resultType="com.xc.auth.entity.SysRole">
        select * from sys_role;
    </select>
    <insert id="addRole" parameterType="com.xc.auth.entity.SysRole">
        insert into sys_role(roleName) values(#{roleName})
    </insert>
</mapper>