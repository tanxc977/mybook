<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xc.auth.mapper.UserMapper">
    <select id="findAll"  resultType="com.xc.auth.entity.SysUser">

        SELECT * from sys_user ;
    </select>

    <insert id="addUser" parameterType="com.xc.auth.entity.SysUser">
        insert into sys_user(username, password) values(#{username}, #{password})
    </insert>
</mapper>